{
  "name": "nestjs-profiler",
  "version": "1.0.22",
  "description": "A NestJS module for profiling HTTP requests, database queries, and cache operations. Inspired by Symfony Profiler, it provides a web-based dashboard to inspect request duration, executed queries, log messages, and explain plans for slow queries.",
  "author": "Mohamed Raslan",
  "private": false,
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.js"
    }
  },
  "files": [
    "dist/libs/nestjs-profiler",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "nest build && copyfiles -u 1 libs/nestjs-profiler/src/assets/**/* dist/assets",
    "build:lib": "rm -rf dist && tsc -p libs/nestjs-profiler/tsconfig.lib.json && cp libs/nestjs-profiler/package.json dist/nestjs-profiler/ && cp README.md dist/nestjs-profiler/ && mkdir -p dist/nestjs-profiler/assets dist/nestjs-profiler/views && cp -r libs/nestjs-profiler/src/assets/* dist/nestjs-profiler/assets/ && cp -r libs/nestjs-profiler/src/views/* dist/nestjs-profiler/views/ && mv dist/nestjs-profiler/src/* dist/nestjs-profiler/ 2>/dev/null || true && rm -rf dist/nestjs-profiler/src",
    "deploy": "npm run build:lib && cd dist/nestjs-profiler && npm publish",
    "prepublishOnly": "npm run build:lib",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "nestjs-profiler",
    "performance",
    "monitoring",
    "debugging",
    "backend",
    "nodejs",
    "telescope",
    "profiler"
  ],
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "cache-manager": "^7.2.8",
    "copyfiles": "^2.4.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "(/|^)test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "libs/nestjs-profiler/src/**/*.(t|j)s",
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@nestjs/cache-manager": ">=2.0.0",
    "@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "cache-manager": ">=5.0.0",
    "mongodb": ">=4.0.0",
    "mysql2": ">=2.0.0",
    "pg": ">=8.0.0",
    "rxjs": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "@nestjs/cache-manager": {
      "optional": true
    },
    "cache-manager": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MohammedRaslan/Nest-JS-Profiler.git"
  },
  "bugs": {
    "url": "https://github.com/MohammedRaslan/Nest-JS-Profiler/issues"
  },
  "homepage": "https://github.com/MohammedRaslan/Nest-JS-Profiler#readme",
  "publishConfig": {
    "access": "public"
  }
}