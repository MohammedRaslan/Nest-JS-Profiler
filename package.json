{
  "name": "nestjs-profiler",
  "version": "1.0.0",
  "description": "A NestJS module for profiling HTTP requests, database queries, and cache operations. Inspired by Symfony Profiler, it provides a web-based dashboard to inspect request duration, executed queries, log messages, and explain plans for slow queries.",
  "author": "Mohamed Raslan",
  "private": false,
  "license": "MIT",
  "main": "./dist/libs/nestjs-profiler/index.js",
  "types": "./dist/libs/nestjs-profiler/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/libs/nestjs-profiler/index.d.ts",
      "require": "./dist/libs/nestjs-profiler/index.js",
      "import": "./dist/libs/nestjs-profiler/index.js"
    }
  },
  "files": [
    "dist/libs/nestjs-profiler",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "nest build && copyfiles -u 1 libs/nestjs-profiler/src/assets/**/* dist/assets",
    "build:lib": "nest build nestjs-profiler && cp libs/nestjs-profiler/package.json dist/libs/nestjs-profiler/ && mkdir -p dist/libs/nestjs-profiler/src && cp -r libs/nestjs-profiler/src/assets dist/libs/nestjs-profiler/src/ && cp -r libs/nestjs-profiler/src/views dist/libs/nestjs-profiler/src/ && node -e \"const pkg=require('./dist/libs/nestjs-profiler/package.json'); pkg.main='./src/index.js'; pkg.types='./src/index.d.ts'; require('fs').writeFileSync('./dist/libs/nestjs-profiler/package.json', JSON.stringify(pkg, null, 2))\"",
    "prepublishOnly": "npm run build:lib",
    "format": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "nestjs-profiler",
    "performance",
    "monitoring",
    "debugging",
    "backend",
    "nodejs",
    "telescope",
    "profiler"
  ],
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "(/|^)test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "libs/nestjs-profiler/src/**/*.(t|j)s",
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "rxjs": ">=7.0.0",
    "pg": ">=8.0.0",
    "mongodb": ">=4.0.0",
    "mysql2": ">=2.0.0",
    "@nestjs/cache-manager": ">=2.0.0",
    "cache-manager": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "pg": { "optional": true },
    "mongodb": { "optional": true },
    "mysql2": { "optional": true },
    "@nestjs/cache-manager": { "optional": true },
    "cache-manager": { "optional": true }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MohammedRaslan/Nest-JS-Profiler.git"
  },
  "bugs": {
    "url": "https://github.com/MohammedRaslan/Nest-JS-Profiler/issues"
  },
  "homepage": "https://github.com/MohammedRaslan/Nest-JS-Profiler#readme",
  "publishConfig": {
    "access": "public"
  }
}
